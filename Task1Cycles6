public class Task1Cycles6 {
    public static void main(String[] args) {

        //Вывести на экран соответствий между символами и их численными обозначениями в памяти компьютера

        for (int i=0; i<=255; i++){
            char a= (char) i;
            System.out.print(i+" - "+a+"\t\t");
            if (i%10==9){
                System.out.print("\n");
            }
        }
    }
}
